<h3>Team Activity for project <b><%= @project.name %></b></h3>
<br/>

<div class="bs-example">
  <div class="tab-content">

    <div id="current-dev-list" class="col-md-4">
      <div class="panel panel-info">
        <div class="panel-heading">Current Developer List</div>
        <div class=" panel-body">
          <%= render partial: "current_developer_list" %>
        </div>
      </div>
    </div>

    <div id="assignment-activity" class="col-md-8">
      <div class="panel panel-info">
        <div class="panel-heading">Assignment Activities</div>
        <div class=" panel-body">
          <%= render partial: "assignment_activites" %>
        </div>
      </div>
    </div>
  </div>
</div>

<div class="col-md-12">
<%= link_to 'Back', projects_path %>
</div>



