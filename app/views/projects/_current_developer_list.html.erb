<table class="table table-striped">
    <thead>
    <tr>
      <th>Developer Name</th>
      <th>Participation Percentage</th>
    </tr>
    </thead>
    <tbody>
    <tbody>
    <% @team_members.each do |team| %>

        <tr>
          <td class="<%= team.developer.active ? '' : 'inactive-user'%>"><%= team.developer.name %> </td>
          <td class="<%= team.developer.active ? '' : 'inactive-user'%>"><%= team.participation_percentage %>%</td>
        </tr>
    <% end %>
    </tbody>
</table>

<div class="pagination_table">
    <%= will_paginate @team_members, renderer: BootstrapPagination::Rails, :param_name => 'developer_page', :params => { :tab_name => "developer_list" } %>
</div>