<div class="panel-body">
  <table class="table table-striped">
    <thead>
    <tr>
      <th>User Email</th>
      <th>Action</th>
    </tr>
    </thead>
    <tbody>
    <tbody>
    <% users.each do |user| %>
        <tr class='user_<%= user.id %>' data-id="<%= user.id %>">
          <td><%= user.super_admin? ? user.email + ' (Super Admin)' : user.email %> </td>
          <% unless user.super_admin? %>
              <td><%= link_to 'Upgrade to Admin', make_user_admin_admin_path(id: user.id), data: {confirm: 'Are you sure to upgrade into Admin ?'}, remote: true %> </td>
          <% else %>
              <td></td>
          <% end %>
        </tr>
    <% end %>
    </tbody>
  </table>

  <div class="pagination_table">
    <%= will_paginate users, renderer: BootstrapPagination::Rails, :param_name => 'user_pagination', :params => {:tab_name => "users_list"} %>
  </div>
</div>